{
  "require": {
    "php": "7.0.*",
    "lizards-and-pumpkins/catalog": "0.9.0-alpha",
    "lizards-and-pumpkins/lib-key-value-store-filesystem": "0.6.0-alpha",
    "lizards-and-pumpkins/lib-search-engine-filesystem": "0.4.0-alpha",
    "lizards-and-pumpkins/lib-queue-filesystem": "0.7.0-alpha",
    "lizards-and-pumpkins/lib-image-processing-imagick": "0.5.0-alpha"
  },
  "require-dev": {
    "phpunit/phpunit": "5.4.*",
    "lizards-and-pumpkins/coding-standards": "dev-master",
    "league/climate": "^3.2.0"
  },
  "autoload": {
    "psr-4": {"LizardsAndPumpkins\\": "src/lizards-and-pumpkins/src"}
  },
  "autoload-dev": {
    "psr-4": {
      "LizardsAndPumpkins\\": [
        "vendor/lizards-and-pumpkins/catalog/tests/Unit/Util",
        "tests/Unit/Suites"
      ]
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit -c tests/Unit/phpunit.xml.dist"],
    "sniff": "vendor/bin/phpcs --runtime-set ignore_warnings_on_exit true --runtime-set ignore_errors_on_exit true -p --colors --encoding=utf-8 --standard=vendor/lizards-and-pumpkins/coding-standards/src/LizardsAndPumpkins/ src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
